{
  "name": "@carretto/projection",
  "version": "0.0.3",
  "private": false,
  "description": "Utilities for projection",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc --build",
    "check:types": "tsc --noEmit",
    "test": "pnpm check:types && pnpm unit",
