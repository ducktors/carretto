    "test:watch": "tsx --watch --test ./test/*.ts",
    "test:coverage": "c8 --all --src dist --reporter lcov --reporter text tsx --test ./test/*.ts"
  },
  "keywords": [],
  "author": "Alessandro Magionami - @alemagio",
  "license": "MIT",
  "dependencies": {
    "dot-object": "^2.1.5",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@types/dot-object": "^2.1.6",
    "rimraf": "^6.0.1"
  },
  "files": ["dist"]
}
